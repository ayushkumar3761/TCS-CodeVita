#include<bits/stdc++.h>
using namespace std;
void sum(int arr[],int n){
    int sum1=0;
    for(int i=0;i<n;i++){
        sum1+=arr[i];
    }
    cout<<sum1;
}
int main(){
    int n1;
    cout<<"enter the size of an array:"<<endl;
    cin>>n1;
    int arr1[n1];
    for(int i=0;i<n1;i++){
        cin>>arr1[i];
    }
    sum(arr1,n1);
}
