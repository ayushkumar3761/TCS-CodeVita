#include<bits/stdc++.h>
using namespace std;
int largestElement(vector<int>& arr){
    int n=arr.size();
    sort(arr.begin(),arr.end());
    return arr[n-1];
}
int main(){
    
    int n;
    cout<<"Enter the size of the array:";
    cin>>n;
    vector<int> arr1(n);
    int x;
    cout<<"enter the array element:"<<endl;
    for(int i=0;i<n;i++){
        cin>>x;
        arr1.push_back(x);
    }
    cout<<largestElement(arr1);
    
    
    
}
