#include <bits/stdc++.h>
using namespace std;
void subset(int s){
    int N=10;
    int k=5;
    if(s>=1 && s<=5){
        cout<<"Number of candy sold:"<< s <<endl;
        cout<<"Number opf candy available:"<<N-s<<" ";
    }
    else{
        cout<<"Invalid input";
    }
}
int main(){
    int n;
    cin>>n;
    subset(n);
}
