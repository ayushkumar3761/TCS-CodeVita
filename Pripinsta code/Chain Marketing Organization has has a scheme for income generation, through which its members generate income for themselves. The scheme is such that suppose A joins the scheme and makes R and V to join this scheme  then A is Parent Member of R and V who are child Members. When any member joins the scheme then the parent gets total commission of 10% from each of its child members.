#include <bits/stdc++.h>
using namespace std;
int main(){
    int schmeamount=5000;
    string pname;
    cin>>pname;
    char ch;
    cin>>ch;
    if(ch=='Y'){
        int n;
        cin>>n;
        string arr[n];
        for(int i=0;i<n;i++){
            cin>>arr[i];
        }
        cout<<"Total members:"<<n+1<<endl;
        cout<<pname<<":"<<n*500<<"INR"<<endl;
        for(int i=0;i<n;i++){
            cout<<arr[i]<<":"<<250<<"INR"<<endl;
        }
        
    }
    else if(ch=='N'){
        cout<<"Number of member is :1"<<endl;
        cout<<"commession:250.0 INR"<<endl;
    }
    
}
